    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/retro_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/retro_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Retroflipz">
        <activity android:name=".activity.SplashActivity">
            <intent-filter> <category android:name="android.intent.category.LAUNCHER" />
                <action android:name="android.intent.action.MAIN" /></intent-filter>
        </activity>
        <activity android:name=".activity.GameshowActivity" />
        <activity android:name=".activity.MainActivity">

        </activity>
    </application>

</manifest>